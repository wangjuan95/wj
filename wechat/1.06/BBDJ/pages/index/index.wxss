.masker {
  position: fixed;
  width: 100%;
  height: 100%;
  top: 0;
  display: flex;
  align-items: center;
  justify-content: center;
  background-color: rgba(0, 0, 0, 0.6);
  color: #fff;
}
.masker view view {
  margin-top: 20rpx;
}

.header {
    margin: 90rpx 0 90rpx 50rpx;
    border-bottom: 1px solid #ccc;
    text-align: center;
    width: 650rpx;
    height: 300rpx;
    line-height: 450rpx;
}

.header image {
    width: 200rpx;
    height: 200rpx;
}

.content {
    margin-left: 50rpx;
    margin-bottom: 90rpx;
}

.content text {
    display: block;
    color: #9d9d9d;
    margin-top: 40rpx;
}

.bottom {
    border-radius: 80rpx;
    margin: 70rpx 50rpx;
    font-size: 35rpx;
}

.model {
  z-index: 23;
  position: fixed;
  top: 50%;
  margin-top: -300rpx;
  width: 600rpx;
  background-color: #fff;
  left: 50%;
  margin-left: -300rpx;
  border-radius: 8px;
}
.modelTitle {
  width: 100%;
  height: 100rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 40rpx;
  color: #333;
  border-bottom: 1rpx solid #f4f5fa;
}
.modelBody {
  width: 100%;
  padding: 50rpx 30rpx;
  box-sizing: border-box;
  font-size: 34rpx;
  line-height: 50rpx;
  color: #909090;
}
.agree {
  color: #01a75b;
  background-color: #fff;
  font-size: 40rpx;
}
.mask {
  position: fixed;
  width: 100%;
  height: 100%;
  background-color: rgba(0, 0, 0, 0.5);
  z-index: 22;
}
.index_banner {
  width: 100%;
  position: relative;
  margin-bottom: 120rpx;
}
.index_banner image {
  display: block;
  width: 100%;
  height: 368rpx;
}
.banner_site {
  position: absolute;
  left: 30rpx;
  top: 60rpx;
  display: flex;
  align-items: center;
}
.banner_site text {
  padding: 0 10rpx;
  display: block;
  float: left;
  font-size: 30rpx;
  color: #fff;
}
.banner_site image {
  float: left;
  display: block;
  width: 30rpx;
  height: 30rpx;
}
.index_banner .banner_us {
  position: absolute;
  width: 690rpx;
  height: 170rpx;
  left: 30rpx;
  bottom: -110rpx;
  background-color: #fff;
  border-radius: 4px;
  overflow: hidden;
  padding: 0 45rpx;
  box-sizing: border-box;
  box-shadow: 0px 10px 5px -5px #ccc,
              5px 0px 5px -5px #ccc,
              -5px 0px 5px -5px #ccc;
}
.index_banner .banner_us > view {
  float: left;
}
.index_banner .banner_us .banner_us_01 {
  margin-top: 35rpx;
  width: 100rpx;
  height: 100rpx;
  /* border-radius: 50%; */
  overflow: hidden;
}
.index_banner .banner_us .banner_us_01 image {
  display: block;
  width: 100%;
  height: 100%;
  border-radius: 50%;
}
.index_banner .banner_us .banner_us_02 {
  height: 170rpx;
  display: flex;
  margin-left: 30rpx;
}
.index_banner .banner_us .banner_us_02 .banner_us_02_ct {
  align-self: center;
}
.index_banner .banner_us .banner_us_02 text {
  display: block;
}
.index_banner .banner_us .banner_us_02 .text1 {
  font-size: 30rpx;
  color: #333;
  font-weight: bold;
}
.index_banner .banner_us .banner_us_02 .text2 {
  margin-top: 6rpx;
  font-size: 24rpx;
  color: #999;
}
.index_banner .banner_us .banner_us_03 {
  float: right;
}
.index_banner .banner_us .banner_us_03 button {
  margin-top: 55rpx;
  font-size: 32rpx;
  height: 60rpx; 
  line-height: 58rpx; 
  color:#01a75b; 
  text-align: center;
  border: 1rpx solid #01a75b;
  background-color: #fff;
}
