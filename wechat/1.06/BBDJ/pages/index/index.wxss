.item1_right {
  /* float: right; */
  display: flex;
  align-items: center;
  justify-content: center;
}
.item1_right image {
  display: block;
  margin: 0 auto;
}
.item1_right image {
  width: 124rpx;
  height: 124rpx;
}
/*  */
.index_container {
  width: 100%;
  padding: 0 30rpx 60rpx;
  box-sizing: border-box;
  overflow: hidden;
}
/* 2 */
.index_container_item2 {
  margin-top: 20rpx;
  width: 100%;
  padding: 0 21rpx 0 21rpx;
  box-sizing: border-box;
  border-radius: 4px;
  /* background-image: url("https://wx2.sinaimg.cn/mw690/005PNriJly1g2bfh2g8maj30j60dwgr4.jpg"); */
  background-size: 100% 100%;
  background-repeat: no-repeat;
}
.index_container_item2_title {
  width: 100%;
  height: 90rpx;
  line-height: 90rpx;
  color: #875b3b;
  font-size: 34rpx;
}
.index_container_item2_title text {
  font-size: 24rpx;
  color: #999;
  padding-left: 20rpx;
}
.index_container_item2_ct {
  width: 100%;
  overflow: hidden;
  padding-bottom: 12rpx;
}
.index_container_item2_ct_item {
  float: left;
  width: 156rpx;
  height: 191rpx;
  line-height: 180rpx;
  background-color: #fff;
  margin-right: 9rpx;
  text-align: center;
  margin-bottom: 9rpx;
  border-radius: 4px;
  position: relative;
}
.index_container_item2_ct_item view {
  font-size: 24rpx;
  color: #0f1c2d;
  height: 60rpx;
  width: 100%;
  line-height: 60rpx;
  position: absolute;
  bottom: 0;
  text-align: center;
}
.index_container_item2_ct_item:nth-child(4n) {
  margin-right: 0;
}
.index_container_item2_ct_item image {
  width: 99rpx;
  height: 55rpx;
}
